		"_COMMENT_serialization_options_" : ["nquads (DEFAULT)", "turtle", "ntriples", "trig", "trix", "jsonld", "hdt"]

	},
	"profile": {
		"type": "tabular",
        "_COMMENT_profile_type_options_" : ["timeseries", "tabular", "raster", "textual", "hierarchical", "rdfGraph"]
	}
}