{
	"input": {
		"files": "datasets/tabular_vector_example.csv",
		"_COMMENT_format_options_" : [".csv", ".txt", ".tif", ".rdf", "..."],
		"sep": "|",
		"header": 0,
		"serialization" : "turtle",
		"_COMMENT_serialization_options_" : ["application/rdf+xml (DEFAULT)", "nquads", "nquads", "turtle", "ntriples", "trig", "trix", "jsonld", "hdt", "..."],
		"_COMMENT_parameters": "Optional parameters for timeseries and tabular cases",
		"light_mode": false,
		"num_cat_perc_threshold": 0.5,
		"max_freq_distr": 10,
		"_COMMENT_ts_parameters": "Parameters for timeseries cases",
		"ts_mode": false,
		"ts_mode_datetime_col" : "date",
		"_COMMENT_tabular_parameters": "Parameters for tabular cases",
		"crs": "EPSG:4326",
		"eps_distance": 1000,
		"extra_geometry_columns" : [{
			"longitude": "lon",
			"latitude": "lat"
		}]
	},
	"output": {
		"json": "output/json/tabular_vector_profile.json",
		"rdf": "output/json/results.rdf",
